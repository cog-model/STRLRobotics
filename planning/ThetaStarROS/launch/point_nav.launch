<launch>
<arg name="x"/>
<arg name="y"/>

<node pkg="theta_star_planner" type="theta_star_planner_node" name="planner" output="screen">
  <param name="agent_radius" value="0.5"/>
  <param name="reach_radius" value="0.4"/>
  <remap from="/map" to="/grid_map"/>
</node>

<node pkg="tx2_action_server" type="navigate_to_xy.py" name="action_server" output="screen">
  <param name="x" value="$(arg x)"/>
  <param name="y" value="$(arg y)"/>
  <param name="tolerance" value="1.5"/>
  <param name="rate" value="2"/>
  <param name="timeout" value="60"/>
  <param name="max_path_fails" value="5"/>
</node>

<include file="$(find controller_node)/launch/control_real.launch"/>
</launch>
